{
  "name": "react-jsx-super-table",
  "version": "1.3.0",
  "description":
    "A lightweight table with search and soring options for a React app which takes regular JSX to render the body.",
  "repository": "github:rnglnd/react-jsx-super-table",
  "main": "lib/index.js",
  "author": "Steven Ringland <steven.ringland@gmail.com>",
  "license": "MIT",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/SuperTable.min.js",
  "scripts": {
    "flow": "flow",
    "start": "webpack-dev-server --mode development",
    "dev": "npm start",
    "test": "jest",
    "build": "babel src -d lib && BABEL_ENV=es babel src -d es",
    "precommit": "npm test && lint-staged",
    "prepublish": "rimraf lib && rimraf es && npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,json}": ["prettier-eslint --write", "git add"]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "./node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/))\\.js?$",
    "moduleFileExtensions": ["js", "json"]
  },
  "keywords": ["react", "component", "table", "grid"],
  "dependencies": {
    "prop-types": "15.6.1"
  },
  "peerDependencies": {
    "react": "^0.14.6 || 15.x.x || 16.x.x",
    "react-dom": "^0.14.6 || 15.x.x || 16.x.x"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-flow-react-proptypes": "22.0.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-15": "1.0.5",
    "enzyme-adapter-react-15.4": "1.0.5",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-unobtrusive": "1.2.2",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "flow-bin": "0.69.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "lint-staged": "7.0.4",
    "prettier": "1.11.1",
    "prettier-eslint": "8.8.1",
    "prettier-eslint-cli": "4.7.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "rimraf": "2.6.2",
    "style-loader": "0.20.3",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.3"
  }
}
