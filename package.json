{
  "name": "react-jsx-super-table",
  "version": "1.0.5",
  "description":
    "A lightweight table for a React app which takes JSX for the body (instead of a config). There&#39;s also config options for searching fields and sorting columns.",
  "main": "lib/index.js",
  "author": "Steven Ringland <steven.ringland@gmail.com>",
  "license": "MIT",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/SuperTable.min.js",
  "scripts": {
    "clean": "rimraf lib && rimraf es",
    "precommit": "npm test && lint-staged",
    "test": "jest",
    "dev": "npm run webpack:watch",
    "webpack:watch":
      "webpack-dev-server --hot --inline --progress --colors --config=webpack.config.js",
    "start": "npm run dev",
    "prepublish": "npm run clean && npm run transpile:lib && npm run transpile:es",
    "transpile:es": "BABEL_ENV=es babel src --out-dir es --copy-files",
    "transpile:lib": "babel src --out-dir lib --copy-files"
  },
  "lint-staged": {
    "*.{js,jsx,json}": ["prettier-eslint --write", "git add"]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "./node_modules/babel-jest"
    },
    "testRegex": "(/test/.*|(\\.|/))\\.js?$",
    "moduleFileExtensions": ["js", "json"]
  },
  "keywords": ["react", "component"],
  "dependencies": {
    "prop-types": "^15.6.0",
    "ramda": "0.25.0",
    "react-debounce-input": "3.2.0"
  },
  "peerDependencies": {
    "react": "^0.14.6 || 15.x.x || 16.x.x",
    "react-dom": "^0.14.6 || 15.x.x || 16.x.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-flow-react-proptypes": "^17.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-adapter-react-15.4": "^1.0.5",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "flow-bin": "^0.65.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.2",
    "jest": "^22.2.2",
    "lint-staged": "^6.1.1",
    "prettier": "^1.5.2",
    "prettier-eslint": "^6.4.1",
    "prettier-eslint-cli": "^4.1.1",
    "prop-types": "^15.6.0",
    "ramda": "0.25.0",
    "react": "^16.2.0",
    "react-debounce-input": "3.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  }
}
