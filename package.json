{
  "name": "react-jsx-super-table",
  "version": "1.3.0",
  "description": "A lightweight table with search and soring options for a React app which takes regular JSX to render the body.",
  "repository": "github:rnglnd/react-jsx-super-table",
  "main": "lib/index.js",
  "author": "Steven Ringland <steven.ringland@gmail.com>",
  "keywords": [
    "react",
    "component",
    "table",
    "grid"
  ],
  "license": "MIT",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/SuperTable.min.js",
  "scripts": {
    "lint": "eslint examples/** --fix",
    "flow": "flow",
    "start": "webpack-dev-server --mode development",
    "dev": "npm start",
    "test": "jest",
    "build": "babel src -d lib && BABEL_ENV=es babel src -d es",
    "precommit": "npm test && lint-staged",
    "prepublish": "rm -rf lib && rm -rf es && npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "./node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/))\\.js?$",
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "dependencies": {
    "prop-types": "15.6.1"
  },
  "peerDependencies": {
    "react": "^0.14.6 || 15.x.x || 16.x.x",
    "react-dom": "^0.14.6 || 15.x.x || 16.x.x"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.2.0",
    "babel-loader": "7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-flow-react-proptypes": "22.0.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-15": "1.0.5",
    "enzyme-adapter-react-15.4": "1.0.5",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.0.1",
    "eslint-config-unobtrusive": "1.2.2",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-react": "7.10.0",
    "flow-bin": "0.75.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "jest": "23.2.0",
    "lint-staged": "7.2.0",
    "prettier": "1.13.7",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "style-loader": "0.21.0",
    "webpack": "4.14.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  }
}
